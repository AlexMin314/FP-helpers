{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins": ["import", "react", "jsx-a11y", "jest"],
  "env": {
    "jest": true
  },
  "rules": {
    "no-else-return": 0,
    "no-confusing-arrow": 0,
    "implicit-arrow-linebreak": 0,
    "import/prefer-default-export": 0,
    "no-nested-ternary": 0
  }
}